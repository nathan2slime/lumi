{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "verbose": true,
  "setupFilesAfterEnv": [
    "<rootDir>/__tests__/jest.setup.ts",
    "<rootDir>/__tests__/mocks/index.ts"
  ]
}
